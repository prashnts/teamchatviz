      "name": "Tobias Lauer",
      "email": "tobias.lauer@moovel.com"
    }
  ],
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@slack/client": "5.0.2",
    "bluebird": "3.7.2",
    "density-clustering": "1.3.0",
    "dotenv": "8.2.0",
    "faker": "3.1.0",
    "kcors": "2.2.0",
    "koa": "2.0.0-alpha.7",
    "koa-basic-auth": "2.0.0",
    "koa-bodyparser": "3.2.0",
    "koa-compress": "2.0.0",
    "koa-conditional-get": "2.0.0",
    "koa-convert": "1.2.0",
    "koa-etag": "3.0.0",
    "koa-generic-session": "1.11.5",
    "koa-mount": "2.0.0",
    "koa-passport": "2.2.2",
